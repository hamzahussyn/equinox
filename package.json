{
  "name": "equinox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:drop && npx sequelize db:create && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-handlebars": "^6.0.6",
    "express-validator": "^6.14.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.2.5",
    "node-redis": "^0.1.7",
    "office-text-extractor": "^1.5.0",
    "rotating-file-stream": "^2.1.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1"
  }
}
